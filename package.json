    "axios": "^0.21.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.6",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
